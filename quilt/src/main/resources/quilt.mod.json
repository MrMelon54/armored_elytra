{
  "schema_version": 1,
  "mixin": [
    "armored_elytra.mixins.json",
    "armored_elytra-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "armored_elytra",
    "version": "${version}",
    "metadata": {
      "name": "Armored Elytra",
      "description": "${description}",
      "contributors": {
        "MrMelon54": "Owner"
      },
      "contact": {
        "homepage": "https://mrmelon54.com/minecraft/armored-elytra",
        "sources": "https://github.com/mrmelon54/armored_elytra",
        "issues": "https://github.com/mrmelon54/armored_elytra/issues"
      },
      "license": "GPL-3.0-only",
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.mrmelon54.ArmoredElytra.quilt.ArmoredElytraQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": "${minecraft_version}"
      },
      {
        "id": "architectury",
        "version": ">=${architectury_version}"
      }
    ]
  },
  "minecraft": {
    "environment": "client"
  }
}
